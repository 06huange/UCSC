@# Not a perl program.
# $Id: html_color.perl-require,v 1.1 2020-07-11 16:28:43-07 - - $

#
# Included by other programs (txt2html, mkheader) to 
# define html colors generated by those programs.
# Must be in the same directory as the source for 
# those programs.
#

our $bg_data_color    = "#2F8F8F"; # dark cyan
our $bg_due_color     = "#8F8F2F"; # dark yellow
our $bg_empty_color   = "#2F2F2F"; # dark gray
